{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Aplicação clone do https://www.tabnews.com.br/ para estudar e criar um pedacinho melhor de internet.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run services:up && next dev",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:stop": "docker compose -f infra/compose.yaml stop",
    "services:down": "docker compose -f infra/compose.yaml down"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "next": "^13.1.6",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.1.0"
  }
}
